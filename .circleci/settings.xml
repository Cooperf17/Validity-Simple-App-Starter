<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
      http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <id>maven-s3-release-repo</id>
            <username>${env.MAVEN_S3_RELEASE_REPO_USERNAME}</username>
            <password>${env.MAVEN_S3_RELEASE_REPO_PASSWORD}</password>
        </server>
        <server>
            <id>maven-s3-snapshot-repo</id>
            <username>${env.MAVEN_S3_SNAPSHOT_REPO_USERNAME}</username>
            <password>${env.MAVEN_S3_SNAPSHOT_REPO_PASSWORD}</password>
        </server>
    </servers>
</settings>
